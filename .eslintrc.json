{
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jquery": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "$": true,
    "Mustache": true,
    "ahoy": true,
    "Highcharts": true
  },
  "rules": {
    "semi": ["error", "always"],
    "indent": ["off"],
    "space-before-function-paren": ["off"],
    "object-curly-spacing": ["off"]
  }
}
